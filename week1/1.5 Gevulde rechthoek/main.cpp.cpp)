#include "hwlib.hpp"
#include "rectangle.hpp"

int main(int argc, char **argv){

   // the window in which we want to print the line
   hwlib::target::window w( hwlib::xy( 512, 256 ));

   filled_rectangle square( w, 10, 10, 30, 30 );
   square.print();

   filled_rectangle square1 ( w, 90, 90, 120, 120 );
   square1.print();


   // keep the window around until we close it
   for(;;){ w.poll(); }
}